{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:meteor/recommended"
  ],
  "settings": {
    "import/resolver": {
      "meteor": {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    }
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-console": 0,
    "prettier/prettier": [
      "error",
      {
        "traillingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ]
  },
  "parser": "babel-eslint",
  "plugins": [
    "prettier",
    "meteor",
    "babel"
  ]
}